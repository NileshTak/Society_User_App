<?xml version="1.0" encoding="utf-8"?>

<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_margin="5dp"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@android:color/white"
        android:baselineAligned="false"
        android:orientation="horizontal"
        android:padding="5dp">

        <TextView
            android:id="@+id/EditProfSocietyname"
            android:layout_width="160dp"
            android:layout_height="wrap_content"

                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
            android:gravity="center_vertical"
            android:text="Society Name"
            android:layout_alignParentLeft="true"
            android:textColor="#0d0c0c"
            android:textSize="16sp" />

        <TextView
                android:id="@+id/EditProfSocietynameAns"
                android:layout_width="150dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:text="Title"
                android:layout_alignParentRight="true"

                android:textColor="#0d0c0c"
                android:textSize="16sp" />

        <TextView
                android:id="@+id/EditProfWingname"
                android:layout_below="@+id/EditProfSocietynameAns"
                android:layout_width="160dp"
                android:layout_height="wrap_content"

                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:text="Wing Name"
                android:layout_alignParentLeft="true"
                android:textColor="#0d0c0c"
                android:textSize="16sp" />

        <TextView
                android:id="@+id/EditProfWingnameAns"
                android:layout_below="@id/EditProfSocietynameAns"
                android:layout_width="120dp"
                android:layout_height="wrap_content"

                android:layout_marginLeft="10dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:text="Title"

                android:layout_alignParentRight="true"
                android:textColor="#0d0c0c"
                android:textSize="16sp" />

        <TextView
                android:id="@+id/EditProfFlatNo"
                android:layout_below="@id/EditProfWingnameAns"
                android:layout_width="160dp"
                android:layout_height="wrap_content"

                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:text="Flat Number"
                android:layout_alignParentLeft="true"
                android:textColor="#0d0c0c"
                android:textSize="16sp" />

        <TextView
                android:id="@+id/EditProfFlatNoAns"
                android:layout_below="@id/EditProfWingnameAns"
                android:layout_width="120dp"
                android:layout_height="wrap_content"

                android:gravity="center_vertical"
                android:text="Title"

                android:layout_marginLeft="10dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"

                android:layout_alignParentRight="true"
                android:textColor="#0d0c0c"
                android:textSize="16sp" />

        <TextView
                android:id="@+id/EditProfMobile"
                android:layout_below="@id/EditProfFlatNoAns"
                android:layout_width="160dp"
                android:layout_height="wrap_content"

                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:text="Mobile Number"
                android:layout_alignParentLeft="true"
                android:textColor="#0d0c0c"
                android:textSize="16sp" />

        <TextView
                android:id="@+id/EditProfMobileAns"
                android:layout_below="@id/EditProfFlatNoAns"
                android:layout_width="120dp"
                android:layout_height="wrap_content"

                android:layout_marginLeft="10dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:text="Title"
                android:layout_alignParentRight="true"
                android:textColor="#0d0c0c"

                android:textSize="16sp" />


        <TextView
                android:id="@+id/EditProfAlterMobile"
                android:layout_below="@id/EditProfMobileAns"
                android:layout_width="140dp"
                android:layout_height="wrap_content"

                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:text="Alternate Mobile Number"
                android:layout_alignParentLeft="true"
                android:textColor="#0d0c0c"
                android:textSize="16sp" />

        <TextView
                android:id="@+id/EditProfAlterMobileAns"
                android:layout_below="@id/EditProfMobileAns"
                android:layout_width="120dp"
                android:layout_height="wrap_content"

                android:layout_marginLeft="10dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:layout_alignParentRight="true"
                android:textColor="#0d0c0c"

                android:textSize="16sp" />

        <TextView
                android:layout_below="@id/EditProfAlterMobile"
                android:layout_width="120dp"
                android:layout_height="wrap_content"

                android:layout_marginLeft="5dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                android:gravity="center_vertical"
                android:text="Account Verification"
                android:layout_alignParentLeft="true"
                android:textColor="#0d0c0c"
                android:textSize="16sp" />

        <TextView
                android:id="@+id/EditProfAuthenAns"
                android:layout_below="@id/EditProfAlterMobile"
                android:layout_width="120dp"
                android:layout_height="wrap_content"


                android:layout_marginLeft="20dp"
                android:layout_marginRight="5dp"
                android:layout_marginTop="5dp"

                android:gravity="center_vertical"
                android:layout_alignParentRight="true"
                android:textColor="#0d0c0c"
                android:textSize="16sp" />



    </RelativeLayout>

</androidx.cardview.widget.CardView>